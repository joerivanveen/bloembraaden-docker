{
  "DB_MAIN": {
    "host": "postgres",
    "port": "5432",
    "name": "$POSTGRES_DB",
    "user": "$POSTGRES_USER",
    "pass": "$POSTGRES_PASSWORD",
    "schema": "public"
  },
  "DB_HISTORY": {
    "host": "postgres",
    "port": "5432",
    "name": "$POSTGRES_DB_HISTORY",
    "user": "$POSTGRES_USER",
    "pass": "$POSTGRES_PASSWORD",
    "schema": "public"
  },
  "MAIL": {
    "active": "mailchimp",
    "mailchimp": {
      "api_key": "$MAILCHIMP_API_KEY",
      "api_url": "https://mandrillapp.com/api/1.0/"
    },
    "sendgrid": {
      "api_key": "$SENDGRID_API_KEY",
      "api_url": "https://api.sendgrid.com/v3/"
    },
    "mailgun": {
      "api_key": "$MAILGUN_API_KEY",
      "api_url": "https://api.eu.mailgun.net/v3/"
    }
  },
  "integrations": {
    "pdfmaker": {
      "api_key": "$PDFMAKER_API_KEY",
      "api_url": "https://chrome.browserless.io/pdf"
    }
  },
  "newrelic_records_backend": false,
  "max_memory_limit": "1g",
  "frame_ancestors": "$FRAME_ANCESTORS",
  "VERBOSE": true,
  "dbcache": "/tmp/cache/",
  "cdnpath": "/mnt/static/htdocs/",
  "cdnroot": "https://static.$MAIN_URL/",
  "logfile": "/tmp/logs/bloembraaden_",
  "uploads": "/mnt/uploads/",
  "invoice": "/mnt/invoice/",
  "version": "$BLOEMBRAADEN_VERSION",
  "install": $BLOEMBRAADEN_INSTALL
}
